{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "EnableNewUserBucketCreation",
      "Effect": "Allow",
      "Action": [
        "s3:CreateBucket",
        "s3:DeleteBucket",
        "s3:ForceDeleteBucket",
        "s3:PutBucketVersioning"
      ],
      "Resource": [
        "arn:aws:s3:::*"
      ]
    },
    {
      "Sid": "EnableBucketMetrics",
      "Effect": "Allow",
      "Action": [
        "admin:Prometheus"
      ]
    }
  ]
}
