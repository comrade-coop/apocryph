{
	"containers": [
		{
			"name": "blogger",
			"image": {
				"url": "chentex/random-logger:latest"
			},
			"ports": [
				{
					"name": "internal",
					"containerPort": 80,
					"hostHttpHost": "guestbook.localhost"
				}
			],
			"env": {
				"GET_HOSTS_FROM": "env",
				"REDIS_MASTER_SERVICE_HOST": "redis",
				"REDIS_SLAVE_SERVICE_HOST": "redis"
			},
			"resourceRequests": [
				{
					"resource": "cpu",
					"amountMillis": 100
				},
				{
					"resource": "memory",
					"amount": 100000000
				}
			]
		},
		{
			"name": "bepis",
			"image": {
				"url": "docker.io/library/redis:alpine"
			},
			"resourceRequests": [
				{
					"resource": "cpu",
					"amountMillis": 10
				},
				{
					"resource": "memory",
					"amount": 100000000
				}
			]
		}
	],
	"replicas": {
		"min": 1,
		"max": 1
	}
}
