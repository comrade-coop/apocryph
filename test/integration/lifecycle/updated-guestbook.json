{
  "containers": [
    {
      "name": "js-redis",
      "image": {
        "url": "hello-world:latest"
      },
      "ports": [
        {
		  "name":"internal",
          "containerPort": 99,
          "hostHttpHost": "guestbook.localhost"
        }
      ],
      "env": {
        "GET_HOSTS_FROM": "env",
        "REDIS_MASTER_SERVICE_HOST": "redis",
        "REDIS_SLAVE_SERVICE_HOST": "redis"
      },
      "resourceRequests": [
        {
          "resource": "cpu",
          "amountMillis": 100
        },
        {
          "resource": "memory",
          "amount": 100000000
        }
      ]
    },
    {
      "name": "bepis",
      "image": {
        "url": "hello-world:latest"
      },
      "resourceRequests": [
        {
          "resource": "cpu",
          "amountMillis": 10
        },
        {
          "resource": "memory",
          "amount": 100000000
        }
      ]
    }
  ],
  "replicas": {
    "min": 0,
    "max": 1
  }
}
