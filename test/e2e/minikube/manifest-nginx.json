{
  "containers": [
    {
      "name": "test-container",
      "image": {
        "url": "docker.io/nginxdemos/nginx-hello:latest"
      },
      "ports": [
        {
          "containerPort": 8080,
          "hostHttpHost": "example.local"
        }
      ],
      "resourceRequests": [
        {
          "resource": "cpu",
          "amountMillis": 10
        },
        {
          "resource": "memory",
          "amount": 100000000
        }
      ]

    }
  ],
  "replicas": {
    "min": 0,
    "max": 1
  }
}
